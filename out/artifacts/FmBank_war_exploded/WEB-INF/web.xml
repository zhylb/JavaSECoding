<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.lihd.web.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/loginServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>GetAllServlet</servlet-name>
        <servlet-class>com.lihd.web.GetAllServlet</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>ChangeInfoServlet</servlet-name>
        <servlet-class>com.lihd.web.ChangeInfoServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>ChangeInfoServlet</servlet-name>
        <url-pattern>/changeInfoServlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>GetAllServlet</servlet-name>
        <url-pattern>/getAllServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>RegistServlet</servlet-name>
        <servlet-class>com.lihd.web.RegistServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegistServlet</servlet-name>
        <url-pattern>/registServlet</url-pattern>
    </servlet-mapping>
</web-app>